      "max": "${path} field must be at earlier than ${max}"
    },
    "array": {
      "required": "${path} обов'язково поле",
      "default": "${path} is invalid'",
      "min": "${path} field must have at least ${min} items",
      "max": "${path} field must have less than or equal to ${max} items"
    }
